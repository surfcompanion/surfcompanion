{
	"name": "Surf Companion",
	"version": "0.0.1",
	"manifest_version": 2,
	"background": {
		"scripts": ["js/backgrounds/hot-reload.js"]
	},
	"content_scripts": [
		{
			"matches": ["*://*.instagram.com/*"],
			"js": ["js/sites/instagram.com.js"]
		},
		{
			"matches": ["*://*.youtube.com/*"],
			"js": ["js/sites/youtube.com.js"]
		}
	]
}
